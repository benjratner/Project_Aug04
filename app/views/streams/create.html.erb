<marquee <marquee behavior="scroll" scrollamount="5" direction="left" width="100%">
<% @tweets.each do |f| %>
	<a href=<%= f.url %>>
		<div class="tweet-item">
			<img class="user-img" src=<%= f.img_url %>></img>
			<span class="username"><%= f.screen_name %></span><br>
			<span class="tweet-content"><%= f.text %></span>
		</div>
	</a>
<% end %>
</marquee>

<%= link_to 'Back Home', root_path %>